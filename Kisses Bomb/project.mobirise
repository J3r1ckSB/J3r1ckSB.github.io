br-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"media-container-row\">\n            <div class=\"col-md-{{logoSize}} col-6\">\n                <div class=\"media-wrap\">\n                    <a href=\"index.html\">\n                        <img src=\"@PROJECT_PATH@/assets/images/kbomb-1.webp\" alt=\"Mobirise Website Builder\">\n                    </a>\n                </div>\n            </div>\n            <div class=\"col-{{12-logoSize}} col-6\">\n                <p class=\"mbr-text align-right links mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".links\"><b class=\"mobile\"><a href=\"policies.html#content01-2q\" class=\"text-white text-primary\"><span style=\"transition: 0.5s !important;\">PRIVACIDAD</span></a> &nbsp;&nbsp;&nbsp;&nbsp;\n                    <a href=\"policies.html#content01-2r\" class=\"text-white text-primary\"><span style=\"transition: 0.5s !important;\">TÉRMINOS Y CONDICIONES</span></a> &nbsp;&nbsp;&nbsp;&nbsp;\n<a href=\"policies.html#content01-2s\" class=\"text-white text-primary\"><span style=\"transition: 0.5s !important;\">REEMBOLSOS E INTERCAMBIOS</span></a> &nbsp;&nbsp;&nbsp;&nbsp;\n<a href=\"policies.html#content01-2t\" class=\"text-white text-primary\"><span style=\"transition: 0.5s !important;\">ENVÍOS</span></a></b>\n                </p>\n            </div>\n        </div>\n        <!-- <div class=\"media-container-row\">\n            <div class=\"col-md-12\"> -->\n        <div class=\"footer-lower\" mbr-if=\"showCopyright\">\n            <div class=\"media-container-row\">\n                <div class=\"col-md-12\">\n                    <hr>\n                </div>\n            </div>\n            <div class=\"media-container-row\">\n                <div class=\"col-md-6 copyright\">\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-the